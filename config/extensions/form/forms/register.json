{
	"fields":{
		"h2":{
			"tag":"h2",
			"class":"text-normal padding-0-0-20",
			"text":"{{this->form_title}}",
			"html":true
		},
		"first_name":{
			"required":true,
			"class":"row-100",
			"pclass":"padding-0-0-10"
		},
		"last_name":{
			"required":true,
			"class":"row-100",
			"pclass":"padding-0-0-10"
		},
		"birthday":{
			"required":true,
			"class":"row-100",
			"pclass":"padding-0-0-10"
		},
		"category":{
			"required":true,
			"tag":"checkboxes",
			"text":"{{this->select_category}}",
			"class":"row-100",
			"pclass":"padding-0-0-10",
			"options":"{{this->select_categories}}"
		},
		"mail":{
			"required":true,
			"type":"email",
			"class":"row-100",
			"pclass":"padding-0-0-10"
		},
		"pass":{
			"required":true,
			"encode":true,
			"type":"password",
			"class":"row-100",
			"pclass":"padding-0-0-10"
		},
		"confirm":{
			"tag":"confirm",
			"class":"confirm padding-0-0-10",
			"html":true,
			"private":true
		},
		"submit":{
			"tag":"button",
			"type":"submit",
			"value":"{{this->button_register}}",
			"private":true
		}
	},
	"form":{
		"class":"register-form",
		"method":"post",
		"action":"ajax.php?type=register&todo=submit"
	},
	"table":false,
	"uploaded_dir":false,
	"mail-report":false
}