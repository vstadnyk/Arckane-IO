{
	"fields":{
		"a":{
			"tag":"a",
			"text":"{{this->switch_form}}",
			"href":"{{this->switch_form_href}}",
			"data-name":"register:switchForm",
			"html":true
		},
		"h2":{
			"tag":"h2",
			"class":"text-normal padding-0-0-20",
			"text":"{{this->form_title}}",
			"html":true
		},
		"mail":{
			"required":false,
			"type":"email",
			"class":"row-100",
			"pclass":"padding-0-0-10"
		},
		"pass":{
			"required":false,
			"encode":true,
			"type":"password",
			"class":"row-100",
			"pclass":"padding-0-0-10"
		},
		"confirm":{
			"tag":"confirm",
			"class":"confirm padding-0-0-10",
			"html":true,
			"private":true
		},
		"submit":{
			"tag":"button",
			"type":"submit",
			"pclass":"align-center",
			"value":"{{this->button_register}}",
			"private":true
		}
	},
	"form":{
		"class":"register-form",
		"method":"post",
		"action":"ajax.php?type=register&todo=submit",
		"data-name":"form:form"
	},
	"table":false,
	"uploaded_dir":false,
	"mail-report":false
}