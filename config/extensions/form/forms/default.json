{
	"fields":{
			"controller":{
				"type":"hidden",
				"value":"form",
				"private":true
			},
			"todo":{
				"type":"hidden",
				"value":"send",
				"private":true
			},
			"parent":{
				"type":"hidden",
				"value":"{{this->parent}}"
			},
			"date":{
				"type":"hidden",
				"value":"{[time()]}"
			},
			"confirm":{
				"tag":"confirm",
				"class":"confirm",
				"html":true,
				"private":true
			},
			"fio":{
				"placeholder":false,
				"id":"fio",
				"required":true
			},
			"phone":{
				"placeholder":false
			},
			"mail":{
				"type":"email",
				"required":true,
				"id":"mail",
				"placeholder":false
			},
			"message":{
				"tag":"textarea",
				"required":true,
				"id":"message",
				"placeholder":false
			},
			"submit":{
				"tag":"button",
				"type":"submit",
				"private":true
			}
		},
		"form":{
			"method":"post",
			"action":"/ajax.php",
			"class":"ajax"
		},
		"table":"form",
		"uploaded_dir":"/files/",
		"files_keep_names":false,
		"mail-report":"default.eml"
}